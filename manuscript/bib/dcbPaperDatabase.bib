@INPROCEEDINGS{Barclay:2003a,
  author = {P.~ J.~ Barclay and M.~ P.~ Hayes and P.~ T.~ Gough},
  title = {Reconstructing Seafloor Bathymetry with a Multi-Frequency, Multi-Channel
	Broadband InSAS using Belief Propagation},
  booktitle = {MTS/{IEEE} OCEANS Conf.},
  year = {2003},
  volume = {4},
  pages = {2149--2154},
  address = {San Diego, CA},
  month = {Sep.},
  abstract = {Synthetic Aperture Sonar (SAS) provides high resolution imagery with
	range independent resolution. However, imagery has been limited to
	two-dimensions, showing acoustic reflectivity for a given scene.
	To further enhance the usefulness of a SAS system this needs to be
	combined with a topographical height map of the same scene. This
	paper presents a novel technique to obtain the third-dimension by
	first remapping on to a set of common ground-planes, with the most
	likely surface through this volume found using Belief Propagation.
	This technique allows any a priori information to be incorporated
	and thus provides a maximum a posteriori (MAP) estimate of the seafloor
	height. Results are presented from a rough seafloor simulator with
	height reconstructions using this technique.},
  file = {:Barclay_2003a - Reconstructing Seafloor Bathymetry with a Multi-Frequency, Multi-Channel Broadband InSAS using Belief Propagation.pdf:PDF},
  keywords = {interferometry; wideband; SAS; belief propagation},
  local-url = {file://localhost/Users//Users/dcb19/Dropbox/Papers/Barclay/Reconstructing%20Seafloor%20Bathymetry%20with%20a%20Multi-Frequency,.pdf}
}

