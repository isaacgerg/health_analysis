  title = {A new interpolation kernel for {SAR} interferometric registration},
  journal = {{IEEE} Trans. Geosci. Remote Sensing},
  year = {2003},
  volume = {41},
  pages = {1105--1110},
  number = {5},
  month = {May},
  abstract = {Estimation of digital elevation maps from electromagnetic signals
	received at an interferometric synthetic aperture radar (SAR) requires
	significant processing. In this framework, a key step is the interferometric
	registration, which can be mathematically framed as an interpolation
	procedure. In this letter, a new interpolation kernel for SAR interferometric
	registration is presented and discussed in comparison with five classical
	ones. It is based on the Knab sampling window that has been successfully
	employed in various electromagnetic problems and which recently has
	been proposed as capable to best preserve the stochastic information.
	The numerical experiments show the superiority of the new interpolation
	kernel.},
  annote = {Based on Hanssen:1999 but uses Knab kernel.},
  file = {:Migliaccio_2003a - A new interpolation kernel for SAR interferometric registration.pdf:PDF},
  keywords = {interpolation},
  local-url = {file://localhost/Users//Users/dcb19/Dropbox/Papers/Migliaccio/A%20new%20interpolation%20kernel%20for%20SAR%20interferometric%20registration.pdf}
}

